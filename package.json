{
  "name": "meme-agent",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "setup": "node scripts/ensure-logs-dir.js && pnpm install",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup.ps1",
    "setup:unix": "sudo bash scripts/setup.sh",
    "build": "tsc",
    "start": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "db:setup": "ts-node src/infrastructure/database/setup.ts",
    "db:migrate": "typeorm migration:run",
    "db:revert": "typeorm migration:revert",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --no-cache --testEnvironment=node",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "docs": "typedoc --out docs src",
    "clean": "rimraf dist coverage docs",
    "prestart": "node scripts/ensure-logs-dir.js",
    "benchmark": "autocannon -c 100 -d 40 -p 10 localhost:3000",
    "analyze": "madge --circular --extensions ts src/index.ts",
    "load-test": "artillery run tests/load/scenarios.yml",
    "test:redis": "ts-node scripts/test-redis.ts",
    "test:redis:watch": "nodemon --exec ts-node scripts/test-redis.ts",
    "fix-imports": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --project tsconfig-esm.json scripts/fix-imports.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.2",
    "@jup-ag/core": "^4.0.0-beta.21",
    "@pythnetwork/client": "^2.17.0",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "@switchboard-xyz/oracle": "^2.1.13",
    "amqplib": "^0.10.3",
    "axios": "^1.6.5",
    "bs58": "^5.0.0",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphql": "^16.8.1",
    "helius-sdk": "^1.2.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "openai": "^4.24.7",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "technicalindicators": "^3.1.0",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/amqplib": "^0.10.4",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "artillery": "^2.0.3",
    "autocannon": "^7.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.7",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
